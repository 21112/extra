import random

number = random.randint(1,100)
print(number)

user_number = None
count = 0

levels = {1: 10, 2: 5, 3: 3}
lev = int(input("Выберете уровень 1/ 2/ 3: "))
max_count = levels[lev]
user_count = int(input("Введите колличество участников: "))
users =[]
is_winner = False
winner_name = None
for i in range(user_count):
    user_name = input(f"Введите имя участника {i} ")
    users.append(user_name)
while not is_winner:
    count +=1
    if count > max_count:
        print("Вы проиграли")
        break
    print(f"Попытка № {count}")
    for user in users:
        print (f"Ход {user} ")
        user_number = int(input("Введите число: "))
        if user_number == number:
            is_winner = True
            winner_name = user    
        elif user_number < number:
            print("Ваше число меньше загаданного") 
        else:
            print("Ваше число больше загаданного")
else:
    print(f"Победа {winner_name}")
