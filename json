import json

my_favourite_group = {"name": "Г.М.О.","tracks": "[ Последний месяц осени, Шапито]", "Albums": [{ "name": "Делать панк-рок","year": "2016"},
{"name": "Шапито","year": "2014"}]}

with open("file.json", "w", encoding="utf-8") as f:
    json.dump(my_favourite_group, f)

with open("file.json", "r", encoding="utf-8") as f:
    res = json.load(f)    
print (res)
